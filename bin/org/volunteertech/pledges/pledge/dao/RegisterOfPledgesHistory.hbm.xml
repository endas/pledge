<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="org.volunteertech.pledges.pledge.dao">
	<class name="RegisterOfPledgesHistory" table="REGISTEROFPLEDGESHISTORY">
		<id name="id" column="REGISTEROFPLEDGESHISTORYID">
            <generator class="native"/>
        </id>
        <property name="registerOfPledgesID" type="long" column="REGISTEROFPLEDGESID"/>
        
        <property name="createdByID" type="long" column="CREATEDBYID"/>
        <property name="updatedByID" type="long" column="UPDATEDBYID"/>
        <property name="dateCreated" type="timestamp" column="DATECREATED"/>
        <property name="dateUpdated" type="timestamp" column="DATEUPDATED"/>
    </class>
</hibernate-mapping>

